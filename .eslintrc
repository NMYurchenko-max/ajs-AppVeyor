{
    "env": {
    "browser": true,
    "es6": true,
    "jest": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent": "off",
        "quotes": "off",
        "object-curly-newline": ["error", { "multiline": true, "consistent": true }],
        "no-plusplus": "off",
        "no-console": "off",
        "no-mixed-operators": "off" // Отключить правило no-console
    }
}